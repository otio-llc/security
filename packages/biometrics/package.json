{
  "name": "@trans-stat/biometrics",
  "description": "Node.js esm wrapper for iValt biometric authentication.",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/trans-stat/tas-security.git",
  "publishConfig": {
    "@trans-stat:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "development": {
        "import": "./dist/esm/index.js",
        "require": "./dist/index.min.cjs"
      },
      "production": {
        "import": "./dist/index.min.mjs",
        "require": "./dist/index.min.cjs"
      },
      "types": "./dist/dts/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:tsc": "tsc -p ./tsconfig.json",
    "build:bundle-esm": "esbuild src/index.ts --format=esm --bundle --minify --outfile=dist/index.min.mjs --conditions=development,production",
    "build:bundle-cjs": "esbuild src/index.ts --format=cjs --bundle --minify --sourcemap --outfile=dist/index.min.cjs --conditions=development,production",
    "build": "npm run clean && npm run build:tsc && npm run build:bundle-esm && npm run build:bundle-cjs"
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
